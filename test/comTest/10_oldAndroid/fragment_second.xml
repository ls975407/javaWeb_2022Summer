<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/back_screen_dual"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SecondFragment">

    <ImageView
        android:id="@+id/second_image_02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/dual_image_01"
        android:src="@mipmap/ic_launcher_round"
        android:scaleType="matrix" />

    <ImageView
        android:id="@+id/second_image_01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/dual_image_02"
        android:src="@mipmap/ic_launcher_round"
        android:scaleType="matrix" />


    <TextView
        android:id="@+id/second_label_01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="default"
        android:textSize="20sp" />


</androidx.constraintlayout.widget.ConstraintLayout>